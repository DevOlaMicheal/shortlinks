<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs') %>
<body>

    <%- include('./partials/nav.ejs') %>

    <div class="container">
      <div class="my-5">
        <table class="table table-striped table-hover lead fw-normal">
          <thead>
            <tr>
              <th scope="col">#</th>
              <th scope="col">Details</th>
             
            </tr>
          </thead>
          <tbody>
            <tr>
              <th scope="row fw-normal">Original Url</th>
              <td><%= shrinked.longUrl%> </td>
              
            </tr>
            <tr>
              <th scope="row">Short Url</th>
              
              <td><a href="<%= shrinked.shortUrl%>" target="_blank"> <%= shrinked.shortUrl%> </a> &nbsp; <i class="fa fa-edit"></i> </td>
              
          
            </tr>
            <tr>
              <th scope="row">Unique Tracking id</th>
              
              <td><%= shrinked.uniqueId%> </td>
              
              
            </tr>
            <tr>
              <th scope="row">Number of clicks/visits</th>
              <td><%= shrinked.clicks%></td>
              
            </tr>
          </tbody>
        </table>

      </div>

      <div class="text-center">
        <a href="" class="btn btn-success" style="background-color: #13747d;">Refresh!</a>
      </div>
    </div>

<%- include('./partials/footer.ejs') %>
      
</body>
</html>